{
  "name": "react-fabric",
  "description": "Office UI Fabric components built with React and CSS Modules",
  "homepage": "http://kmees.github.io/react-fabric",
  "version": "0.1.7",
  "main": "lib/index.js",
  "license": "MIT",
  "author": {
    "name": "Kevin Mees",
    "email": "kev.mees@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmees/react-fabric.git"
  },
  "bugs": {
    "url": "https://github.com/kmees/react-fabric/issues"
  },
  "keywords": [
    "office",
    "office ui fabric",
    "react",
    "react-component"
  ],
  "dependencies": {
    "classnames": "^2.2.4",
    "hoist-non-react-statics": "^1.0.5",
    "lodash.flatten": "^4.2.0",
    "lodash.identity": "^3.0.0",
    "lodash.isfunction": "^3.0.8",
    "react-prop-types": "^0.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.9",
    "bumped": "^0.9.2",
    "bumped-finepack": "^0.2.3",
    "bumped-terminal": "^0.7.1",
    "coveralls": "^2.11.9",
    "cpx": "^1.3.1",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "enzyme": "^2.3.0",
    "eslint": "^3.0.0",
    "eslint-config-xi": "^5.0.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "node-sass": "^3.6.0",
    "nyc": "^6.6.1",
    "office-ui-fabric": "^2.6.1",
    "postcss-loader": "^0.9.1",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "tap-min": "^1.1.0",
    "tape": "^4.5.1",
    "watch": "^0.19.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-loader": "0.0.1"
  },
  "scripts": {
    "start": "better-npm-run start",
    "test": "better-npm-run test",
    "pretest": "npm run lint",
    "build": "better-npm-run transpile && better-npm-run copy-sass",
    "prebuild": "npm run lint && better-npm-run clean",
    "prepublish": "npm run build",
    "lint": "eslint src/",
    "patch": "npm run release -- patch",
    "release": "bumped release",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "tdd": "better-npm-run tdd"
  },
  "betterScripts": {
    "clean": {
      "command": "rimraf ./lib"
    },
    "copy-sass": {
      "command": "cpx 'src/**/*.scss' ./lib"
    },
    "start": {
      "command": "node ./spec/server.js"
    },
    "transpile": {
      "command": "babel ./src --ignore 'src/**/__tests__' --out-dir ./lib",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "nyc -s node ./bin/run-tests.js",
      "env": {
        "BABEL_ENV": "test"
      }
    },
    "tdd": {
      "command": "watch 'node ./bin/run-tests.js | ${TAP_FORMATTER:-tap-min}' src"
    }
  },
  "peerDependencies": {
    "office-ui-fabric": "^2.6.0",
    "react": "^0.14 || ^15.0.0",
    "react-dom": "^0.14 || ^15.0.0"
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "exclude": [
      "src/**/__tests__"
    ]
  }
}
